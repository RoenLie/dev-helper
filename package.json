{
  "name": "dev-helper",
  "productName": "dev-helper",
  "version": "0.1.1",
  "description": "Dev helper tooling for running scripts and other utilities.",
  "main": "electron/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RoenLie/dev-helper.git"
  },
  "scripts": {
    "start": "concurrently \"npm:svelte-dev\" \"electron-forge start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-start": "sirv public --single",
    "validate": "svelte-check"
  },
  "keywords": [],
  "author": {
    "name": "Kristoffer Roen-Lie",
    "email": "kristofferroenlie@gmail.com"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dev_helper"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "node-powershell": "^4.0.0",
    "sirv-cli": "^1.0.0",
    "svelte-spa-router": "^3.1.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@types/node-powershell": "^3.1.1",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/node": "^14.14.31",
    "autoprefixer": "^10.2.4",
    "concurrently": "^6.0.0",
    "electron": "11.3.0",
    "electron-builder": "^22.9.1",
    "postcss": "^8.2.6",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.32.8",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.0.0",
    "typescript": "^4.1.5"
  }
}